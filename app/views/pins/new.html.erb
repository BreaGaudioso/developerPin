<div class="container-fluid text-center">
<div class="row">
<h1>Add a Pin!</h1>
</div>

<div class="row">
<%= form_for @pin do |f| %>
<%= f.text_field :url, placeholder: :url%><br>
<%= f.text_field :comment, placeholder: :comment %></br>
<%= f.text_field :image, placeholder: :image %></br>
<%= f.submit  "Add me!" %>
<% end %>
</div>
